<%= form_for @photo do |f| -%>
	<p>
		<%= f.label :title -%>
		<%= f.text_field :title -%>
	</p>
	<p>
		<%= f.label :description -%> <br>
		<%= f.text_area :description -%>
	</p>
	<p>
		<%= f.label :file -%>
		<%= f.text_field :file -%>
	</p>
	<p>
		<%= f.label :status -%>
		<%= f.check_box :status %>
	</p>
	<p>
		<%= f.submit nil, class: 'btn btn-primary' -%>
	</p>
<% end -%>